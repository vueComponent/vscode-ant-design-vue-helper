{
	"a-pagination/total": {
		"description": "total number of data items",
		"optionType": "number",
		"defaultValue": "0"
	},
	"a-pagination/defaultCurrent": {
		"description": "default initial page number",
		"optionType": "number",
		"defaultValue": "1"
	},
	"a-pagination/current": {
		"description": "current page number",
		"optionType": "number",
		"defaultValue": "-"
	},
	"a-pagination/defaultPageSize": {
		"description": "default number of data items per page",
		"optionType": "number",
		"defaultValue": "10"
	},
	"a-pagination/pageSize": {
		"description": "number of data items per page",
		"optionType": "number",
		"defaultValue": "-"
	},
	"a-pagination/hideOnSinglePage": {
		"description": "Whether to hide pager on single page",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-pagination/showSizeChanger": {
		"description": "determine whether pageSize can be changed",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-pagination/pageSizeOptions": {
		"description": "specify the sizeChanger options",
		"optionType": "string[]",
		"defaultValue": "['10', '20', '30', '40']"
	},
	"a-pagination/showQuickJumper": {
		"description": "determine whether you can jump to pages directly",
		"optionType": "boolean | object",
		"defaultValue": "false"
	},
	"a-pagination/showTotal": {
		"description": "to display the total number and range",
		"optionType": "(total: number, range: [number, number]) => any",
		"defaultValue": "-"
	},
	"a-pagination/size": {
		"description": "specify the size of Pagination, can be set to small",
		"optionType": "string",
		"defaultValue": "''"
	},
	"a-pagination/simple": {
		"description": "whether to use simple mode",
		"optionType": "boolean",
		"defaultValue": "-"
	},
	"a-pagination/itemRender": {
		"description": "to customize item innerHTML",
		"optionType": "(page: number, type: \"page\" | \"prev\" | \"next\", originalElement: any) => any",
		"defaultValue": "-"
	}
}