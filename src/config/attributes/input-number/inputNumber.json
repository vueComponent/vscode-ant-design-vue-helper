{
	"a-input-number/autoFocus": {
		"description": "get focus when component mounted",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-input-number/defaultValue": {
		"description": "initial value",
		"optionType": "string | number",
		"defaultValue": "-"
	},
	"a-input-number/disabled": {
		"description": "disable the input",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-input-number/formatter": {
		"description": "Specifies the format of the value presented",
		"optionType": "(value: string | number) => string",
		"defaultValue": "-"
	},
	"a-input-number/max": {
		"description": "max value",
		"optionType": "number",
		"defaultValue": "Infinity"
	},
	"a-input-number/min": {
		"description": "mix value",
		"optionType": "number",
		"defaultValue": "Infinity"
	},
	"a-input-number/parser": {
		"description": "Specifies the value extracted from formatter",
		"optionType": "(string: string) => number",
		"defaultValue": "-"
	},
	"a-input-number/precision": {
		"description": "precision of input value",
		"optionType": "number",
		"defaultValue": "-"
	},
	"a-input-number/decimalSeparator": {
		"description": "decimal separator",
		"optionType": "string",
		"defaultValue": "-"
	},
	"a-input-number/size": {
		"description": "width of input box",
		"optionType": "string",
		"defaultValue": "'default'",
		"options": [
			"small",
			"large",
			"default"
		]
	},
	"a-input-number/step": {
		"description": "The number to which the current value is increased or decreased. It can be an integer or decimal.",
		"optionType": "string | number",
		"defaultValue": "1"
	},
	"a-input-number/value": {
		"description": "current value",
		"optionType": "string | number",
		"defaultValue": "-"
	}
}